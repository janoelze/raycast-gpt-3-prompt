{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gpt-3-prompt",
  "title": "GPT-3 Text Prompt",
  "description": "Execute a prompt against OpenAIs davinci-003 model. You'll need to have an OpenAI API key in order to use this extension. You can get one here: https://beta.openai.com/docs/api-reference/authentication.",
  "icon": "command-icon.png",
  "author": "janoelze",
  "categories": [
    "Web",
    "Data"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "openai_api_key",
      "type": "password",
      "required": true,
      "title": "OpenAI API Key",
      "description": "You'll need this in order to send calls to the OpenAI."
    }
  ],
  "commands": [
    {
      "name": "index",
      "title": "GPT-3 Prompt",
      "description": "Execute a text prompt",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "node-fetch": "^3.3.0",
    "openai": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
